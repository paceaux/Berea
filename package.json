  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paceaux/BibleApi/issues"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
